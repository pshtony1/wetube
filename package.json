{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Youtube Clone with Vanilla and NodeJS",
  "author": "Feca",
  "engines": {
    "node": "14.4.0"
  },
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.1.5",
    "autoprefixer": "^10.0.1",
    "aws-sdk": "^2.774.0",
    "axios": "^0.20.0",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.18.3",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^7.0.2",
    "css-loader": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.2",
    "helmet": "^3.15.0",
    "mini-css-extract-plugin": "^1.1.1",
    "mongoose": "^5.10.9",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss": "^8.1.2",
    "postcss-loader": "^4.0.3",
    "pug": "^2.0.3",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.2",
    "webpack": "^5.2.0",
    "webpack-cli": "^3.3.12"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore 'scss'",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "tunnel": "lt --port 4000",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rd /s \"build\"",
    "start": "node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2"
  }
}
